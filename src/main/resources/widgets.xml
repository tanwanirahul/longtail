<pages>


  <form-template>
    <name>resetpassword</name>
    <widget class="data-form" id="resetpassword" action="${req.servicesBase}/resetPassword" formName="resetpassword">
      <field label="DoesNotMatter" fieldName="DoesNotMatter" fieldType="resetPasswordUserid" />
      <field label="DoesNotMatter" fieldName="DoesNotMatter" fieldType="resetPasswordTime" />
      <field label="DoesNotMatter" fieldName="DoesNotMatter" fieldType="resetPasswordHash" />
      <field label="New Password" fieldName="New Password" fieldType="password" />
      <field label="Change Password" fieldName="Change Password" fieldType="submit" />
    </widget>
  </form-template>

  <form-template>
    <name>changepassword</name>
    <widget class="data-form" id="changepassword" action="${req.servicesBase}/changePassword" formName="changepassword">
      <field label="Old Password" fieldName="Old Password" fieldType="oldPassword" />
      <field label="New Password" fieldName="New Password" fieldType="password" />
      <field label="Change Password" fieldName="Change Password" fieldType="submit" />
    </widget>
  </form-template>
  <form-template>
    <name>forgotpassword</name>
    <widget class="data-form" id="forgotpassword" action="${req.servicesBase}/requestResetPassword" formName="forgotpassword">
      <field label="Email" fieldName="Email" fieldType="email" required="true" helpText="Enter the Email Id used for registration" />
      <field label="Reset my Password" fieldName="Reset my Password" fieldType="submit" />
    </widget>
  </form-template>
  <form-template>
    <name>loginform</name>
    <widget class="data-form" id="loginform" action="/j_spring_security_check" formName="loginform">
      <field label="Email" fieldName="Email" fieldType="userId" required="true" helpText="Enter the Email Id to be used for registration" />
      <field label="Password" fieldName="Password" fieldType="password" required="true" helpText="Your password (5 or more characters)" />
      <field label="Remember me on this computer" fieldName="Remember me on this computer" fieldType="rememberme" required="false" />
      <field label="Login" fieldName="Login" fieldType="submit" />
    </widget>
  </form-template>
  <form-template>
    <name>contactus</name>
    <widget class="data-form" id="contactus" formName="contactus">
      <field label="Name" fieldName="Name" fieldType="name" placeholder="Your Full Name" required="true" helpText="Enter your full name" />
      <field label="Email" fieldName="Email" fieldType="email" placeholder="email@domain.com" required="true" helpText="Enter your email id" />
      <field label="Phone_No" fieldName="Phone_No" fieldType="phone" helpText="Enter your phone (Optional)" />
      <field label="Comments" fieldName="Comments" fieldType="comments" required="true" helpText="Enter your comments/feeback" />
      <field label="Submit Your Request" fieldName="Submit Your Request" fieldType="submit" />
    </widget>
  </form-template>
  <form-template>
    <name>createmysite</name>
    <widget class="data-form" id="createmysite" formName="createmysite">
      <field label="Name" fieldName="Name" fieldType="name" placeholder="Your Full Name" required="true" helpText="Enter your full name" />
      <field label="Email" fieldName="Email" fieldType="email" placeholder="email@domain.com" required="true" helpText="Enter your email id" />
      <field label="Phone_No" fieldName="Phone_No" fieldType="phone" placeholder="Phone Number" required="true" helpText="Enter your business contact number" />
      <field label="Tell Us About Your Business" fieldName="Tell Us About Your Business" placeholder="Business Name, Contact Address, About Your Business and Business Owner, etc" fieldType="comments" required="true" helpText="Your business name, contact address and other details" />
      <field label="Design my Website" fieldName="Design my Website" fieldType="submit" />
    </widget>
  </form-template>

  <form-template>
    <name>signup</name>
    <widget class="data-form" id="signup" action="${req.servicesBase}/site/createAccountAndBlankSite" formName="signup">
      <field label="Name" fieldName="Name" fieldType="name" placeholder="Your Name" required="true" helpText="Enter your full name" />
      <field label="Email" fieldName="Email" fieldType="email" placeholder="email@domain.com" required="true" helpText="We hate spam too. Your email is safe with us." />
      <field label="New Password" fieldName="New Password" fieldType="password" required="true" helpText="Use at least 5 character password." />
      <!--<field label="Retype Password" fieldName="Retype Password" fieldType="confirmPassword" required="true" />-->
      <field label="InviteCode" fieldName="InviteCode" fieldType="inviteCode" required="true" helpText="Don't have one? Request an Invite now!" />
      <!--
        <field label="Industry" fieldName="Industry" fieldType="industry" required="true" /> <field label="Domain" fieldName="Domain" fieldType="domainSelector" required="true" />
      -->
      <field label="Register Me" fieldName="Register Me" fieldType="submit" />
    </widget>
  </form-template>

  <form-template>
    <name>autosignup</name>
